snippet log
	UE_LOG(LogTemp, Log, TEXT("${1:hello world}"));
snippet logi
	UE_LOG(LogTemp, Log, TEXT("${1:var}: %d"), $1);
snippet logf
	UE_LOG(LogTemp, Log, TEXT("${1:var}: %f"), $1);
snippet logs
	UE_LOG(LogTemp, Log, TEXT("${1:var}: %s"), *$1);
snippet logv
	UE_LOG(LogTemp, Log, TEXT("${1:var}: %s"), *$1.ToString());
